{
    "name": "hose1021/devhub",
    "type": "project",
    "description": "The DevHub.",
    "keywords": [
        "social"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4.1",
        "barryvdh/laravel-ide-helper": "^2.6",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "graham-campbell/markdown": "^13.1",
        "guzzlehttp/guzzle": "^6.3",
        "htmlmin/htmlmin": "^7.0",
        "intervention/image": "^2.5",
        "jacobbennett/laravel-http2serverpush": "^5.0",
        "jeremykenedy/laravel-logger": "^3.2",
        "ksubileau/color-thief-php": "^1.4",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.2",
        "laravel/ui": "^2.0",
        "laravelcollective/html": "^6.0",
        "league/commonmark": "^1.0",
        "league/commonmark-ext-table": "^2.0",
        "mews/purifier": "3.*",
        "parsedown/laravel": "^1.1",
        "renatomarinho/laravel-page-speed": "^1.8",
        "sentry/sentry-laravel": "1.8.0",
        "silber/bouncer": "v1.0.0-rc.8",
        "spatie/laravel-responsecache": "^6.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "facade/ignition": "^2.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "pestphp/pest": "^0.2.0",
        "phpunit/phpunit": "^9.0",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
